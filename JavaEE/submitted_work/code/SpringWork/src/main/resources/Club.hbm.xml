<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.springwork.entity.Club" table="club" schema="javaee">
        <id name="clubId" column="club_id"/>
        <property name="logo" column="logo"/>
        <property name="description" column="description"/>
        <property name="name" column="name"/>
        <property name="memberNumber" column="member_number"/>
        <property name="expenditure" column="expenditure"/>
        <property name="phoneNumber" column="phone_number"/>
    </class>
</hibernate-mapping>