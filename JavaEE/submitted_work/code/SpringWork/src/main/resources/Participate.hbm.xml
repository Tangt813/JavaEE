<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.springwork.entity.Participate" table="participate" schema="javaee">
        <composite-id mapped="true" class="com.example.springwork.entity.ParticipatePK">
            <key-property name="userId" column="user_id"/>
            <key-property name="activityId" column="activity_id"/>
        </composite-id>
    </class>
</hibernate-mapping>