<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.springwork.entity.Member" table="member" schema="javaee">
        <composite-id mapped="true" class="com.example.springwork.entity.MemberPK">
            <key-property name="userId" column="user_id"/>
            <key-property name="clubId" column="club_id"/>
        </composite-id>
        <property name="level" column="level"/>
        <property name="clubCost" column="club_cost"/>
    </class>
</hibernate-mapping>